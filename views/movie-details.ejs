<%- include("partials/header.ejs") %>
<h2><%= foundMovie.title %></h2>
    <div class="movie-details row">
        <div class="col-md-6">
            <img src="<%= foundMovie.bannerUrl %>" alt="<%= foundMovie.title %>" id="image-on-details-view">
        </div>
        <div class="col-md-6">
            <ul>
                <% foundMovie.showtimes.forEach(showtime => { %>
                    <li><%= showtime %></li>
                <% }) %>
            </ul>
        </div>
    </div>

<%- include("partials/footer.ejs") %>