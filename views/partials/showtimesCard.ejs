<div class="showtimes-card">
    <h3 class="movie-title"><%= movie.title %></h3>
    <div class="row">
        <div class="col-md-6">
            <img src="<%= movie.posterUrl %>" alt="<%= movie.title %>">
        </div>
        <div class="col-md-6">
            <ul class="showtimes-list">
                <% for(let showdate of Object.keys(showtimesByDate)){ %>
                    <li><%= showdate %>
                        <ul class="showtime-times-ul">
                            <% for(let showtime of showtimesByDate[showdate]){ %>
                                <li class="showtime-time"><%= showtime %></li>
                            <% } %>
                        </ul>
                    </li>
                <% } %>
            </ul>
        </div>
    </div>
</div>